#include<iostream>
using namespace std;
int swape(int arr[],int i ,int j)

{int temp;


	  temp= arr[i];
	arr[i]=arr[j];
	 arr[j]=temp;}
int dnf(int arr[],int n)
{
	int low=0;
	int mid=0;
	int high=n-1;
	while(mid<=high)
	{
		if(arr[mid]==0)
		{swape(arr,low,mid);
			low++;mid++;
			
		}
		else if(arr[mid]==1)
		{
			
			mid++;
		}
		else{
			swape(arr,mid,high);
			high--;
		}
	
	}
	
}
int main(){
	int arr[]={1,2 ,0 ,1 ,2 ,1};
	dnf(arr,6);
	for(int i=0;i<6;i++)
	{
		cout<<arr[i];
	}
	return 0;
}
